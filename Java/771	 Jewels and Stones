class Solution {
    public int numJewelsInStones(String J, String S) 
    {
    	int[] chars = new int['z' + 1];
    	int result = 0;
    	
    	for( char i : J.toCharArray() )
    	{
    		chars[ i ] = 1;
    	}
    	
    	for( char i : S.toCharArray() )
    	{
    		result += chars[ i ]==1?1:0;
       	}
    	
        return result;
    }     
}
